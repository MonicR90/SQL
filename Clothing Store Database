CREATE TABLE clothing (id INTEGER PRIMARY KEY, name TEXT, aisle INTEGER, price INTEGER, material TEXT);

INSERT INTO clothing VALUES (1, "Shirts", 2,12, "Sheer");
INSERT INTO clothing VALUES (2, "Pants", 4,10, "High Waist");
INSERT INTO clothing VALUES (3, "Shorts", 6,8, "Low Rise");
INSERT INTO clothing VALUES (4, "Socks", 8,6, "Cotton");
INSERT INTO clothing VALUES (5, "Shoes", 10,7, "Sneakers");

SELECT *FROM clothing
WHERE price <=12
ORDER BY price desc;


